[version]
0.131.0

[ignore]
.*/github/.*
.*/__tests__/.*
.*/dist/.*
.*/tmp/.*
.*/vendor/.*
.*/public/.*
.*/.git/.*
.*/storage/.*
*/config/.*
.*/.bin/.*
.*/lib/.*
.*/db/.*
.*/spec/.*

[include]
./node_modules/react

[options]
all=false
log.file=./log/flow.log
module.file_ext=.scss
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.json
module.name_mapper='src' -> '<PROJECT_ROOT>/client/src'
module.name_mapper.extension='.scss' -> 'empty/object'
module.ignore_non_literal_requires=false
module.use_strict=false
munge_underscores=true
server.max_workers=1
traces=0
sharedmemory.log_level=1

