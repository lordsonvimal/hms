{
  "name": "hms",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --env.env=dev",
    "build": "webpack --config webpack.config.js --env.env=prod",
    "build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "flow": "flow",
    "lint": "eslint --ext .jsx,.js client/src/",
    "stylelint": "stylelint **/*.scss --config .stylelintrc.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9"
  ],
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-flow": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.2.1",
    "dotenv-webpack": "2.0.0",
    "eslint": "7.6.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "file-loader": "6.1.0",
    "flow-bin": "0.131.0",
    "flow-webpack-plugin": "1.2.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "import-glob-loader": "1.1.0",
    "node-sass-glob-importer": "5.3.2",
    "postcss-loader": "3.0.0",
    "sass": "1.26.10",
    "sass-loader": "9.0.3",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-scss": "3.18.0",
    "stylelint-webpack-plugin": "2.1.0",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "5.1.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "axios": "0.19.0",
    "lodash": "4.17.20",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0"
  }
}
