    "@babel/preset-flow": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "autoprefixer": "10.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.0",
    "eslint": "7.12.0",
    "eslint-loader": "4.0.2",
